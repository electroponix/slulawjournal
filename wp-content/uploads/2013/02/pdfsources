function checkContactForm() {
	var form = document.ContactForm
	var boolSuccess
	var reqtype
	var boolSuccess = true;
	
	
	if (form.Contact[0].checked && (form.ContactEmail.value != permEmail || form.ContactOffice.value != permOffice || form.ContactPhone.value != permPhone )) {
		alert("You are not able to change your permanent address here.  Please check the second (temporary) contact button")
		return (boolSuccess = false);
		}
	if (form.Contact[2].checked && form.ContactName.value == "") {
		alert("Please enter the name of the person we should contact.")
		return (boolSuccess = false);
		}		
	return boolSuccess;
}
