<script language="javascript" type="text/javascript">// <![CDATA[
$(document).ready( function() {
	$('.clearOnFocus').focus( function() {
		var hint_text  = $(this).attr('hint_text');
		var hint_color = $(this).attr('hint_color');

		if( $(this).val() == hint_text ) {
			$(this).val('');
			$(this).css("color","black");	
		}
	});

	$('.clearOnFocus').blur( function() {
		var hint_text  = $(this).attr('hint_text');
		var hint_color = $(this).attr('hint_color');

		if( $(this).val() == '' || $(this).val() == hint_text ) {
			$(this).val( hint_text ); 
			$(this).css("color",hint_color);
		}
	});

	$('.clearOnFocus').blur();

	});
// ]]></script>
